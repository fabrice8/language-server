{
  "compilerOptions": {
    "lib": ["es2017"],
    "pretty": true,
    "target": "es5",
    "sourceMap": true,
    "declaration": true,
    "stripInternal": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true
  }
}
